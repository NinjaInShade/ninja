{
    "name": "@ninjalib/docs",
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "start": "concurrently -n server,client -c green,blue -t=\"HH:mm:ss\" -p=\"[{name}:{time}]\" \"npm:start-server\" \"npm:start-client\"",
        "start-client": "vite",
        "start-server": "ninja-cli node file=./src/server/server.ts",
        "build": "vite build",
        "preview": "vite preview"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^2.0.4",
        "concurrently": "^8.0.1",
        "svelte": "^3.58.0",
        "svelte-check": "^3.1.4",
        "svelte-preprocess": "^5.0.3",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4",
        "vite": "^4.2.1"
    },
    "dependencies": {
        "@ninjalib/build": "^0.1.9",
        "@ninjalib/sql": "^0.2.1",
        "@ninjalib/svelte": "^0.0.5"
    }
}
